							"            pm.collectionVariables.set(\"account_id\", account.id)",
							"        }",
							"    })",
							"});",
							"",
							"pm.sendRequest({",
							"    url: pm.variables.get(\"base_url\") + \"/account/\" + pm.variables.get(\"account_id\"),",
